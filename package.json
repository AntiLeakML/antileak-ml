{
  "name": "antileak-ml",
  "displayName": "AntiLeak ML",
  "description": "Detection of overlapping and pre-processing data leakages that may occur in the Python code of machine learning projects",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:python",
    "onLanguage:json"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "antileak-ml.helloWorld",
        "title": "Hello World"
      }
    ],
    "colors": [
      {
        "id": "myextension.pandasHighlight",
        "description": "Couleur de fond pour les mots 'pandas'",
        "defaults": {
          "dark": "#00FF0055",
          "light": "#00FF0055",
          "highContrast": "#00FF0055"
        }
      },
      {
        "id": "myextension.numpyHighlight",
        "description": "Couleur de fond pour les mots 'numpy'",
        "defaults": {
          "dark": "#80008055",
          "light": "#80008055",
          "highContrast": "#80008055"
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
